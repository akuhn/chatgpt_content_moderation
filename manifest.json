{
    "manifest_version": 3,
    "name": "Unhinged ChatGPT",
    "version": "1.0.0",
    "description": "Relaxed content moderation, inappropriate content is flagged instead of hidden.",
    "content_scripts": [
        {
            "js": ["content.js"],
            "matches": [
                "https://chat.openai.com/*",
                "https://chat.openai.com/"
            ],
            "run_at": "document_start",
            "world": "MAIN"
        }
    ]
}
